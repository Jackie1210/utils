{
  "name": "@clym/utils",
  "version": "0.1.0",
  "description": "utils of clym",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rollup -wc",
    "build": "rimraf dist && cross-env NODE_ENV=production rollup -c",
    "prepublishOnly": "pnpm build",
    "release": "bump --commit --push --tag && pnpm publish",
    "test": "jest",
    "format": "prettier --write .",
    "lint": "eslint src/**/*.ts"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jackie1210/utils.git"
  },
  "keywords": [
    "utils"
  ],
  "author": "clym",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "src/**/*.ts": [
      "eslint --ext .ts"
    ]
  },
  "bugs": {
    "url": "https://github.com/Jackie1210/utils/issues"
  },
  "homepage": "https://github.com/Jackie1210/utils#readme",
  "devDependencies": {
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@swc-node/jest": "^1.4.3",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.16",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "chalk": "^5.0.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.21",
    "eslint": "^8.8.0",
    "eslint-define-config": "^1.2.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.1",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "yorkie": "^2.0.0"
  }
}
